<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="com.mbc.chew.detail.store.StoreService">
    <select id="getStoreByCode" resultType="com.mbc.chew.detail.store.StoreDTO" parameterType="int">
      SELECT storename
      FROM Store_3
      WHERE storecode = #{storecode}
    </select>
   
  
    </mapper>
